    "import sys\n",
    "import unicodedata\n",
    "\n",
    "digitmap = { c: ord('0') + unicodedata.digit(chr(c))\n",
    "for c in range(sys.maxunicode) if unicodedata.category(chr(c)) == 'Nd' }\n",
    "print(len(digitmap))\n",
    "460\n",
    "# Arabic digits\n",
    "x = '\\u0661\\u0662\\u0663wos我'\n",
    "res = x.translate(digitmap)\n",
    "print(res)"
   ]
