    "import socket\n",
    "\n",
    "\n",
    "def get_ip_address():\n",
    "    \"\"\"\n",
    "    查询本机ip地址\n",
    "    :return: ip\n",
    "    \"\"\"\n",
    "    try:\n",
    "        s = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)\n",
    "        s.connect(('8.8.8.8', 80))\n",
    "        ip = s.getsockname()[0]\n",
    "    finally:\n",
    "        s.close()\n",
    "\n",
    "    return ip\n",
    "\n",
    "get_ip_address()"
   ]
